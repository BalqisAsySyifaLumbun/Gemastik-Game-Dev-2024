;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Game"
run/main_scene="res://Storyline/1_Main Menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/show_image=false
config/icon="res://Sprites/Main Characters/sketch.png"

[autoload]

ZIndexManager="*res://Utility/ZIndexManager.gd"
TransitionScreen="*res://Utility/transition_screen.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Otan": "res://Dialogue/Characters/Otan.dch",
"Ucing": "res://Dialogue/Characters/Ucing.dch",
"unit_test_character": "res://Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={
"empty_timeline": "res://Dialogue/Timelines/empty_timeline.dtl",
"gudang": "res://Dialogue/Timelines/gudang.dtl",
"intro_main_room": "res://Dialogue/Timelines/intro_main_room.dtl",
"ke_atas": "res://Dialogue/Timelines/ke_atas.dtl",
"ke_bawah": "res://Dialogue/Timelines/ke_bawah.dtl",
"ke_koridor_utama": "res://Dialogue/Timelines/ke_koridor_utama.dtl",
"main_room": "res://Dialogue/Timelines/main_room.dtl",
"senter_nyala": "res://Dialogue/Timelines/senter_nyala.dtl",
"susun_kotak": "res://Dialogue/Timelines/susun_kotak.dtl",
"terjebak": "res://Dialogue/Timelines/terjebak.dtl",
"otan": "res://Dialogue/Timelines/main_room.dtl",
"papan_tulis": "res://Dialogue/Timelines/intro_main_room.dtl",
"unable_door": "res://Dialogue/Timelines/unable_door.dtl",
"wrong_door": "res://Dialogue/Timelines/wrong_door.dtl"
}
layout/style_list=["res://Dialogue/speaker_textbox.tres"]
layout/default_style="res://Dialogue/speaker_textbox.tres"
glossary/default_case_sensitive=true
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

